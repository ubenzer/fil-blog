.pagination__large-navigation,
.pagination__mobile-navigation
  list-style none
  margin 10px 16px
  justify-content space-between
  user-select none

.pagination__large-navigation
  display none
  +gt-sm()
    display flex
.pagination__mobile-navigation
  display flex
  +gt-sm()
    display none

.pagination__ellipsis
  opacity 0.7
  display flex
  justify-content center
